<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.rxjy.rxdesign.fragment.volumeroom.VolumeFiveFragment">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="90px">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20px"
                        android:layout_weight="1"
                        android:background="@color/colorWhite"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="20px">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="项目名称"
                            android:textColor="@color/cor595757"
                            android:textSize="28px" />

                        <EditText
                            android:id="@+id/et_new_xiangmumingcheng"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="right"
                            android:hint="请输入"
                            android:textColor="@color/cor595757"
                            android:textColorHint="@color/line"
                            android:textSize="28px" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/shape_vessel_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="20px">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="办公地点"
                            android:textColor="@color/cor595757"
                            android:textSize="28px" />

                        <EditText
                            android:id="@+id/et_new_bangongdidian"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="right"
                            android:hint="请输入"
                            android:textColor="@color/cor595757"
                            android:textColorHint="@color/line"
                            android:textSize="28px" />

                    </LinearLayout>

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="78px"
                    android:layout_marginTop="20px"
                    android:background="@drawable/shape_vessel_bg"
                    android:padding="20px">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/tenancy_term"
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/tv_measure_tenancy_term_end_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:text="       "
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/tv_measure_tenancy_term_time_line"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/tv_measure_tenancy_term_end_time"
                        android:text="--"
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/tv_measure_tenancy_term_start_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/tv_measure_tenancy_term_time_line"
                        android:text="       "
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_projectattribute"
                    android:layout_width="match_parent"
                    android:layout_height="78px"
                    android:layout_marginTop="20px"
                    android:background="@drawable/shape_vessel_bg"
                    android:padding="20px">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="项目类型"
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/tv_projectattribute"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:text="       "
                        android:textColor="@color/cor595757"
                        android:textSize="28px" />

                </RelativeLayout>

                <com.rxjy.rxdesign.custom.TextGridview
                    android:id="@+id/tgv_industrytype"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">

                </com.rxjy.rxdesign.custom.TextGridview>

            </LinearLayout>

        </ScrollView>

    </RelativeLayout>

</LinearLayout>
